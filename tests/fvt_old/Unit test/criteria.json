{
    "name": "Fvt_unittest",
    "revision": 2,
    "project": "key",
    "mode": "decision",
    "rules": [{
        "name": "Unit Test Success (Mocha)",
        "conditions": [{
            "eval": "_mochaTestSuccessPercentage",
            "op": "=",
            "value": 100
        }]
    }, {
        "name": "Unit Test Regression (Mocha)",
        "conditions": [{
            "eval": "_hasMochaTestRegressed",
            "op": "=",
            "value": false
        }]
    }]
}
